<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="obj8645931" name="feed_editionStructureArkXmlKelden" modifiedDate="2020-01-20T13:31:43.487Z" xmlns="urn:schemas-teradp-com:gn4tera">
